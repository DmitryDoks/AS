    "\n",
    "def is_prime (y): \n",
    "    res=True \n",
    "    a=int(pow(y, 0.5)) \n",
    "    for i in range(2,a): \n",
    "        if y%i==0: \n",
    "            res=False \n",
    "    return(res) \n",
    "\n",
    "b=int(input('Введите число')) \n",
    "print(is_prime(b))"
