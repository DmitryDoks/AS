    "def is_prime (a):\n",
    "    if  (a.isdigit() and int(a)>=0 and int(a)<=1000):\n",
    "        a=int (a)\n",
    "        for i in range(2,int(a)):\n",
    "            if a%i==0:\n",
    "                return \"false\"\n",
    "    return \"true\"\n",
    "    \n",
    "\n",
    "print(\"Enter the number\")\n",
    "a=input()\n",
    "print(\"This number is prime:\",is_prime(a))"
