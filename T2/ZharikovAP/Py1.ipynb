    "def is_prime (var):\n",
    "    if  var.isdigit() and int(var)>=0 and int(var)<=1000:\n",
    "        var=int (var)\n",
    "        for i in range(2,int(var**(1/2))):\n",
    "            if var%i==0:\n",
    "                return \"false\"\n",
    "    return \"true\"\n",
    "    \n",
    "\n",
    "\n",
    "a=input()\n",
    "print(is_prime(a))"
