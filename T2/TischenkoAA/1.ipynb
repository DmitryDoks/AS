    "def is_prime(x):\n",
    "    f=1\n",
    "    if x>1000 or x<0:\n",
    "        return None\n",
    "    for i in range(2,x):\n",
    "        if x%i==0:\n",
    "            f=0\n",
    "            break\n",
    "    if f==0:\n",
    "        return False\n",
    "    else: \n",
    "        return True\n",
    "a=int(input(\"Введите число:\")) \n",
    "is_prime(a)\n",
    "        "
