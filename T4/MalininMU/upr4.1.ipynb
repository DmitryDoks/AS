    "def bucketSort(a, n, buckets, m):  \n",
    "    for j in range(m):\n",
    "        buckets[j] = 0\n",
    "    for i in range(n):\n",
    "        buckets[a[i]] += 1\n",
    "    i = 0\n",
    "    for j in range(m):\n",
    "        for k in range(buckets[j]):\n",
    "            a[i] = j\n",
    "            i += 1\n"
