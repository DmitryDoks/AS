    "a=input()\n",
    "a=a.strip()\n",
    "i=0\n",
    "while i<len(a):\n",
    "    if a[i]==' ':\n",
    "        if a[i-1]=='*':\n",
    "            a=a[:i]+a[i+1:]\n",
    "            i-=1\n",
    "        else:\n",
    "            a=a[:i]+'*'+a[i+1:]\n",
    "    i+=1\n",
    "print(a)\n",
    "        \n"
