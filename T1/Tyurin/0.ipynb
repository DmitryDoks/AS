   "source": [
    "d = input(\"Диаметр бака: \") \n",
    "d = int(d) \n",
    "h = input(\"Высота бака: \") \n",
    "h = int(h) \n",
    "S = input(\"Площадь окраски одной банкой: \") \n",
    "S = int(S) \n",
    "\n",
    "import math \n",
    "\n",
    "\n",
    "quantity = (4*math.pi*d*h+(2*math.pi*d**2))/S \n",
    "quantity = int(quantity)+1 \n",
    "print(\"Количество банок: \", quantity)"
